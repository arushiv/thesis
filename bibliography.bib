  journal = {Scientific Reports},
  doi = {10.1038/s41598-017-14389-7},
  author = {Thibodeau, Asa and M{\'a}rquez, Eladio J. and Shin, Dong-Guk and {Vera-Licona}, Paola and Ucar, Duygu},
