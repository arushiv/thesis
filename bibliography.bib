  month = feb,
  year = {2013},
  keywords = {Chromatin,Humans,Polymorphism; Single Nucleotide,Software,Diabetes Mellitus; Type 2,Phenotype,Arthritis; Rheumatoid,Databases; Genetic,Genetic Markers,Histones,Nervous System Diseases},
  pages = {124-130},
  file = {/Users/arushi/Zotero/storage/YUYFYBX9/Trynka et al. - 2013 - Chromatin marks identify critical cell types for f.pdf},
