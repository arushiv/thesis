@article{dimasImpactTypeDiabetes2014,
  title = {Impact of {{Type}} 2 {{Diabetes Susceptibility Variants}} on {{Quantitative Glycemic Traits Reveals Mechanistic Heterogeneity}}},
  volume = {63},
  copyright = {\textcopyright{} 2014 by the American Diabetes Association.. Readers may use this article as long as the work is properly cited, the use is educational and not for profit, and the work is not altered. See http://creativecommons.org/licenses/by-nc-nd/3.0/ for details.},
  issn = {0012-1797, 1939-327X},
  abstract = {Patients with established type 2 diabetes display both {$\beta$}-cell dysfunction and insulin resistance. To define fundamental processes leading to the diabetic state, we examined the relationship between type 2 diabetes risk variants at 37 established susceptibility loci, and indices of proinsulin processing, insulin secretion, and insulin sensitivity. We included data from up to 58,614 nondiabetic subjects with basal measures and 17,327 with dynamic measures. We used additive genetic models with adjustment for sex, age, and BMI, followed by fixed-effects, inverse-variance meta-analyses. Cluster analyses grouped risk loci into five major categories based on their relationship to these continuous glycemic phenotypes. The first cluster (PPARG, KLF14, IRS1, GCKR) was characterized by primary effects on insulin sensitivity. The second cluster (MTNR1B, GCK) featured risk alleles associated with reduced insulin secretion and fasting hyperglycemia. ARAP1 constituted a third cluster characterized by defects in insulin processing. A fourth cluster (TCF7L2, SLC30A8, HHEX/IDE, CDKAL1, CDKN2A/2B) was defined by loci influencing insulin processing and secretion without a detectable change in fasting glucose levels. The final group contained 20 risk loci with no clear-cut associations to continuous glycemic traits. By assembling extensive data on continuous glycemic traits, we have exposed the diverse mechanisms whereby type 2 diabetes risk variants impact disease predisposition.},
  language = {en},
  number = {6},
  journal = {Diabetes},
  doi = {10.2337/db13-0949},
  author = {Dimas, Antigone S. and Lagou, Vasiliki and Barker, Adam and Knowles, Joshua W. and M{\"a}gi, Reedik and Hivert, Marie-France and Benazzo, Andrea and Rybin, Denis and Jackson, Anne U. and Stringham, Heather M. and Song, Ci and {Fischer-Rosinsky}, Antje and Boesgaard, Trine Well{\o}v and Grarup, Niels and Abbasi, Fahim A. and Assimes, Themistocles L. and Hao, Ke and Yang, Xia and Lecoeur, C{\'e}cile and Barroso, In{\^e}s and Bonnycastle, Lori L. and B{\"o}ttcher, Yvonne and Bumpstead, Suzannah and Chines, Peter S. and Erdos, Michael R. and Graessler, Jurgen and Kovacs, Peter and Morken, Mario A. and Narisu, Narisu and Payne, Felicity and Stancakova, Alena and Swift, Amy J. and T{\"o}njes, Anke and Bornstein, Stefan R. and Cauchi, St{\'e}phane and Froguel, Philippe and Meyre, David and Schwarz, Peter E. H. and H{\"a}ring, Hans-Ulrich and Smith, Ulf and Boehnke, Michael and Bergman, Richard N. and Collins, Francis S. and Mohlke, Karen L. and Tuomilehto, Jaakko and Quertemous, Thomas and Lind, Lars and Hansen, Torben and Pedersen, Oluf and Walker, Mark and Pfeiffer, Andreas F. H. and Spranger, Joachim and Stumvoll, Michael and Meigs, James B. and Wareham, Nicholas J. and Kuusisto, Johanna and Laakso, Markku and Langenberg, Claudia and Dupuis, Jos{\'e}e and Watanabe, Richard M. and Florez, Jose C. and Ingelsson, Erik and McCarthy, Mark I. and Prokopenko, Inga and Investigators, on behalf of the MAGIC},
