  pages = {e1006673},
  file = {/Users/arushi/Zotero/storage/VMPDBNZY/Tong et al. - 2017 - Shared regulatory sites are abundant in the human .pdf;/Users/arushi/Zotero/storage/YBUIG54G/article.html}
}

