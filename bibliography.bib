  month = jan,
  year = {2012},
  pages = {1798-1812},
  file = {/Users/arushi/Zotero/storage/NQUNQUUL/Wang et al_2012_Sequence features and chromatin structure around the genomic regions bound by.pdf;/Users/arushi/Zotero/storage/XFNILI3R/1798.html},
