  file = {/Users/arushi/Zotero/storage/VKMB3Q7Y/Barbagallo et al_2014_CEBPA exerts a specific and biologically important proapoptotic role in.pdf;/Users/arushi/Zotero/storage/9C8PVJFF/2333.html},
