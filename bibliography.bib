  keywords = {Type 2 diabetes,Families,Heritability,Quantitative traits},
  pages = {2811},
  file = {/Users/arushi/Zotero/storage/UTCCNDM8/Almgren et al_2011_Heritability and familiality of type 2 diabetes and related quantitative traits.pdf}
