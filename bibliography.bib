  journal = {bioRxiv},
  doi = {10.1101/102392},
  author = {Alasoo, Kaur and Rodrigues, Julia and Mukhopadhyay, Subhankar and Knights, Andrew J. and Mann, Alice L. and Kundu, Kousik and Consortium, Hipsci and Hale, Christine and Dougan, Gordon and Gaffney, Daniel J.},
  month = may,
  year = {2017},
  pages = {102392},
  file = {/Users/arushi/Zotero/storage/DCC76BTV/Alasoo et al_2017_Shared genetic effects on chromatin and gene expression reveal widespread.pdf;/Users/arushi/Zotero/storage/BXUX2HWL/102392.html}
}

