  keywords = {Humans,Animals,Quality Control,Gene Expression Profiling,Sequence Analysis; RNA,High-Throughput Nucleotide Sequencing,Gene Library,Reproducibility of Results,Bias,RNA,Sensitivity and Specificity},
  pages = {1543-1551},
  file = {/Users/arushi/Zotero/storage/UG8J7TLJ/Jiang et al_2011_Synthetic spike-in standards for RNA-seq experiments.pdf},
