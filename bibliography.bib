  year = {2012},
  keywords = {Algorithms,Gene Expression Profiling,Sequence Analysis; RNA,Bayes Theorem,Carcinoma; Squamous Cell,Genetic Variation,High-Throughput Nucleotide Sequencing,Linear Models,Mouth Neoplasms},
  pages = {4288-4297},
  file = {/Users/arushi/Zotero/storage/KMJWRS82/McCarthy et al. - 2012 - Differential expression analysis of multifactor RN.pdf},
  pmid = {22287627},
