  month = jun,
  year = {2018},
  keywords = {eQTLs,gene expression regulation,complex trait,conditional eQTL,expression quantitative trait loci,fine mapping,GWAS co-localization,neuropsychiatric disorder,risk gene,schizophrenia},
  pages = {1169-1184},
