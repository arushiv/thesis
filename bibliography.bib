  author = {Jiang, Hao-Yuan and Wek, Sheree A. and McGrath, Barbara C. and Lu, Dan and Hai, Tsonwin and Harding, Heather P. and Wang, Xiaozhong and Ron, David and Cavener, Douglas R. and Wek, Ronald C.},
