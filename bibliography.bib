~}
}

@article{vallianatosAmeliorationBrainHistone2019,
  title = {Amelioration of {{Brain Histone Methylopathies}} by {{Balancing}} a {{Writer}}-{{Eraser Duo KMT2A}}-{{KDM5C}}},
  copyright = {\textcopyright{} 2019, Posted by Cold Spring Harbor Laboratory. The copyright holder for this pre-print is the author. All rights reserved. The material may not be redistributed, re-used or adapted without the author's permission.},
  abstract = {{$<$}p{$>$}Methylation of histone H3 lysine 4 (H3K4me) is extensively regulated by seven writer- and six eraser enzymes in mammalian cells. Nine of the 13 H3K4me enzymes have been associated with neurodevelopmental disorders to date, indicating their important yet poorly-understood roles in the brain. Opposite activities of writer-eraser enzymes highlight modulation of their activities as a therapeutic strategy. However, interplay among the H3K4me enzymes in the brain remains largely unknown. Here, we show functional interactions of a writer-eraser duo, KMT2A and KDM5C, which are responsible for Wiedemann-Steiner Syndrome (WDSTS), and mental retardation X-linked syndromic Claes-Jensen type (MRXSCJ), respectively. Despite the opposing enzymatic activities, the WDSTS and MRXSCJ mouse models, deficient for either Kmt2a or Kdm5c, shared similar brain transcriptomes, reduced dendritic spines, and increased aggression. The altered H3K4me landscapes and transcriptomes in the single mutants were partially corrected in the Kmt2a-Kdm5c double-mutant (DM) mice. Moreover, the reduced spine density and key behavioral traits including aggression were clearly reversed in DM mice. Thus, our study uncovers common yet mutually-suppressive aspects of WDSTS and MRXCJ and provides a proof of principle for balancing a single writer-eraser pair to ameliorate their associated disorders.{$<$}/p{$>$}},
  language = {en},
  journal = {bioRxiv},
  doi = {10.1101/567917},
  author = {Vallianatos, Christina N. and Raines, Brynne and Porter, Robert S. and Wu, Mike and Garay, Patricia M. and Collette, Katie M. and Seo, Young Ah and Dou, Yali and Keegan, Katherine and Tronson, Natalie and Iwase, Shigeki},
  month = mar,
  year = {2019},
  pages = {567917},
  file = {/Users/arushi/Zotero/storage/K67WHSUP/Vallianatos et al_2019_Amelioration of Brain Histone Methylopathies by Balancing a Writer-Eraser Duo.pdf;/Users/arushi/Zotero/storage/X9J269PB/567917v1.html}
}

