  keywords = {transcription,enhancers,P-TEFb,Pol II pausing,superenhancers,termination},
  pages = {26-41},
  file = {/Users/arushi/Zotero/storage/FLGQQHUV/Henriques et al. - 2018 - Widespread transcriptional pausing and elongation .pdf;/Users/arushi/Zotero/storage/SB2V34XN/26.html},
