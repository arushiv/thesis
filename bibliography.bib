  month = apr,
  year = {2015},
  pages = {381-386},
  file = {/Users/arushi/Zotero/storage/RTC52SHG/Onengut-Gumuscu et al_2015_Fine mapping of type 1 diabetes susceptibility loci and evidence for.pdf;/Users/arushi/Zotero/storage/TKU69VSZ/ng.html}
